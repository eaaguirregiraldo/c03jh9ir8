<h1>Actores</h1>

<table>
  <tr>
    <th>Foto</th>
    <th>Nombre</th>
    <th>Bio</th>
    <th>Fecha Nacimiento</th>
    <th>Lugar Nacimiento</th>
  </tr>
  <% @actors.each do |actor| %>
    <tr>

      <td><img src="<%= actor.image_url %>" alt="<%= actor.name %>"></td>
      <td><%= actor.name %></td>
      <td><%= actor.bio %></td>
      <td><%= actor.birth_date %></td>
      <td><%= actor.birth_place %></td>
    </tr>
  <% end %>
</table>